            local leftLeg = character:FindFirstChild("Left Leg")
            leftLeg.Massless = true
            leftLeg.Size = Vector3.new(Studs, 2, Studs)
            leftLeg.Transparency = 1
            local newLeftLeg = Instance.new("Part")
            newLeftLeg.Name = "Left Leg"
